{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "scripts": {
      "build": "npm run build"
    },
    "framework": "vite",
    "cleanUrls": true,
    "trailingSlash": false,
    "cache": false,
    "github": {
      "enabled": true,
      "autoJobCancelation": true
    }
  }
  